@section LocalJs{
    <script>
        debugger;
        $(document).ready(function () {

            debugger;
            var Location = "@Html.Raw(ViewBag.toAddLeadForm)";
            var storedValue = sessionStorage.getItem('pageId');
            if (storedValue == null) {
                refreshDataTable(0);
            } else {
                var page_Id = parseInt(storedValue);
                refreshDataTable(page_Id);
            }
            
            $("#TxtFilterDate,#TxtScheduleDate").datepicker({
                format: 'dd/mm/yyyy'
            });
            const $select = document.querySelector('#TxtSource');
            $select.value = "@Session["Admin_ID"]";

        });
        function refreshDataTable(pageId) {
            debugger;

            $("#LeadDetails").load("@Url.Action("LeadTablePartial", "LeadManagement")", function () {
                var pageToSet = pageId;
                var myDataTable = $('#TableLead').DataTable({
                });
                if (pageId != 0) {
                    sessionStorage.setItem('pageId', 0);
                }
                myDataTable.page(pageToSet).draw(false);
                $("#TableLead").removeClass('dataTable');
                $(".col-sm-12").addClass('overflow');
                $(".col-sm-12").css('padding-right','0px');
                const $select = document.querySelector('#TxtSource');
                $select.value = "@Session["Admin_ID"]";
                var id = "@Session["Admin_ID"].ToString()";
                getCountOfUnseenNotification(id);
                RecentNotificationDetails(id);
                $('.content-wrapper').css('min-height', '646px');
         });
         
        }
        function FilterDataTable(whereCompanyId, whereCategoryId, whereAssignedToId, wherePriorityListId, whereStatusListId)
        {
	        debugger;
	        $('#FilterLeadDetails').attr('disabled', true);
	        var originalContent = $('#FilterLeadDetails').html();
	        var loader = "<i class='fa fa-spinner fa-spin'></i>Please wait..";
	        $('#FilterLeadDetails').html(loader);
	        debugger;
	        var User_ID = "@Session["Admin_ID"].ToString()";
            if (whereCompanyId == "" && whereCategoryId == "" && whereAssignedToId == "" && wherePriorityListId == "" && whereStatusListId == "") {
		    refreshDataTable(0);
		    $('#FilterLeadDetails').removeAttr('disabled');
		    $("#Filter-Detail .close").click();
		    $('#FilterLeadDetails').html(originalContent);
	        }
	        else {
		        var formdata = new FormData();
		        formdata.append("UserId", User_ID);
		        formdata.append("CompanyId", whereCompanyId);
		        formdata.append("CategoryId", whereCategoryId);
		        formdata.append("AssignedId", whereAssignedToId);
                formdata.append("Priority", wherePriorityListId);
                formdata.append("Status", whereStatusListId);
		        $.ajax({
			    url: '@Url.Action("FilterLeadTablePartial", "LeadManagement")',
			    type: 'POST',
			    data: formdata,  // Send the FormData object directly
			    contentType: false, // Let jQuery set the content type
			    processData: false, // Don't process the data
			    success: function (data) {
			    	$("#LeadDetails").html(data); // Replace with the element where you want to display the result
			    	$("#TableLead").DataTable();
			    	$('#FilterLeadDetails').removeAttr('disabled');
			    	$("#Filter-Detail .close").click();
			    	$('#FilterLeadDetails').html(originalContent);
			    },
			    error: function (error) {
				    console.log(error);
			    }
		    });
	    }

    }
    </script>
