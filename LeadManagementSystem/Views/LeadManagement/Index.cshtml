<style>
    .dataTable > thead > tr > th[class*="sort"]:before,
    .dataTable > thead > tr > th[class*="sort"]:after {
        content: "" !important;
    }

    .downloadtemp {
        font-weight: 600;
    }

    textarea#TxtRemark {
        height: 35px;
    }

    a#linkdownload {
        cursor: pointer;
    }

    .caret {
        display: none;
    }

    span.multiselect-selected-text {
        display: flex;
        margin-left: 4px;
    }

    span .multiselect-selected-text {
        margin-right: 10rem;
    }

    .btnsave {
        display: inline !important;
    }


    .btn-default:hover, .btn-default:active {
        background-color: transparent;
    }

    .btn-default:hover {
        border-color: none;
    }

    .btn-default {
        background-color: white;
        color: #444;
        border-color: #ddd;
    }

    button.multiselect.dropdown-toggle.btn.btn-default {
        width: 50rem;
        max-width: 100%;
        border-radius: 0px;
    }

    .btn-group, .btn-group-vertical {
        max-width: 100%;
    }

    .bortext {
        border: 1px solid red;
    }

    .form-group-chk {
        margin-bottom: 0px;
        display: inline-table;
        width: 100%;
        padding: 0px;
    }

    .control-label-o {
        margin-top: 0px;
        display: contents;
    }

    .input-group .form-control, .input-group-addon, .input-group-btn {
        display: table-cell;
    }

    .form-control-t {
        display: block;
        width: 8rem;
        padding: 5px;
    }

    input.form-control-t {
        text-align: center;
    }

    span.input-group-addon {
        padding: 6px 12px;
        font-size: 14px;
        font-weight: 400;
        line-height: 1;
        color: #555;
        text-align: center;
        background-color: #eee;
        border: 1px solid #ccc;
        border-radius: 4px;
        border-color: #d2d6de;
        background-color: #fff;
    }

    .radiogrp {
        display: flex;
        margin-top: 5px;
    }

    .TDSCls {
        margin-left: 15px;
        display: inline-block;
    }

    .col-md-4.boxtds {
        margin-top: -5px;
    }

    .labelvalue {
        margin-bottom: 4px;
    }

    .input-group {
        position: relative;
        display: table;
        border-collapse: separate;
    }

    a#btnAddLead {
        padding: 6px 20px;
    }

    #TxtFilterDate {
        height: 33px;
        margin-top: 7px;
        width: 195px;
        margin-right: 5px;
    }

    .RemarkDatahead {
        font-family: 'Rubik', sans-serif;
        font-weight: 400;
        padding-left: 13px;
        padding-top: 5px;
    }

    .RemarkDatadate {
        font-family: 'Rubik', sans-serif;
        font-weight: 400;
        color: #808080;
        padding-left: 13px;
    }

    .RemarkDataDes {
        font-family: 'Rubik', sans-serif;
        text-align: left;
        color: #333333;
        font-size: 14px;
        width: 90%;
        margin-left: 13px;
        margin-bottom: 8px;
    }

    .RemarkData {
        border: 1px solid #e4e4e4;
        border-radius: 16px;
        box-shadow: 0px 4px 8px #e4e4e4;
    }

    .d-flex p {
        margin-bottom: 5px;
    }

    .RemarkData_Block {
        margin-top: 10px;
        border: 1px solid #e4e4e4;
        border-radius: 16px;
        box-shadow: 0px 4px 8px #e4e4e4;
    }

    .modal-dialog-scrollable {
        max-height: calc(100% - 3.5rem);
    }
    #FrontFileStatus{
        width:20px;
    }
