    /* Style the tooltip text */
    .tooltip-text {
        width: 120px;
        visibility: hidden;
        background-color: #3c8dbc;
        border: 1px solid#3c8dbc;
        color: white;
        text-align: center;
        border-radius: 5px;
        padding: 5px;
        position: absolute;
        bottom: 115%;
        left: 50%;
        transform: translateX(-50%);
        transition: visibility 0.2s ease-in-out;
    }

        .tooltip-text::after {
            content: "";
            position: absolute;
            top: 100%;
            left: 50%;
            margin-left: -10px;
            border-width: 9px;
            border-style: solid;
            border-color: #3c8dbc transparent transparent transparent;
        }
    /* Show the tooltip text on button hover */
    #nevigateToEditClientDetailsDiv:hover .tooltip-text {
        visibility: visible;
    }

    #nevigateToEditContactDetailsDiv:hover .tooltip-text {
